{
  "expo": {
    "name": "Zona Segura",
    "slug": "zona-segura",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/iconLabel.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/iconLabel.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zonasegura.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/iconLabel.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.zonasegura.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/images/iconLabel.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir $(PRODUCT_NAME) a usar sua localização."
        }
      ]
    ],
    "scheme": "zonasegura",
    "newArchEnabled": true
  }
}
