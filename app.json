{
  "expo": {
    "name": "Zona Segura",
    "slug": "zonaSegura",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/iconLabel.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "updates": {
      "url": "https://u.expo.dev/7ca53f9a-8f50-4acb-88e0-cee2a60fd84d",
      "enabled": false,
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ihd.zonaSegura"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/iconLabel.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ihd.zonaSegura"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/iconLabel.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/iconLabel.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7ca53f9a-8f50-4acb-88e0-cee2a60fd84d"
      }
    }
  }
}
